                                    <script>#!/bin/bash
env &gt; /tmp/env.sh
set -e
export PATH=$PATH:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/opt/groovy-1.8.6/bin
export JAVA_HOME=/usr/java/jre-vmware
export HOME=/var/lib/rabbitmq
cls_nodes=(${hosts_name[@]})
###
### Stop the RabbitMQ server - erlang.cookie must be created now
### Enable RabbitMQ Management Plug-in
###
service rabbitmq-server stop
rabbitmq-plugins enable rabbitmq_management

###
### Replace the cookie in all nodes
###
echo $erlang_cookie &gt; /var/lib/rabbitmq/.erlang.cookie

###
### add IP addresses to hosts - ignore the current node
###
for ((i=0; i&lt;${#hosts_name[@]}; i++))
do
     if [[ $node_index -ne $i ]];
     then
         echo ${hosts_ip[$i]} ${hosts_name[$i]}  &gt;&gt; /tmp/hosts
     fi
done

sed -i.bak -e "/localhost\|$myhostname/! s/^[0-9].*//g" /etc/hosts
cat /tmp/hosts &gt;&gt; /etc/hosts

###
### If first node - start rabbitmq 
### otherwise add other nodes as slaves to the first
###
if [ $node_index -eq 0 ];
then
    service rabbitmq-server start    
fi

if [ $node_index -gt 0 ];
then
   sleep 60
   service rabbitmq-server start
   rabbitmqctl stop_app
   rabbitmqctl reset
   rabbitmqctl cluster rabbit@${hosts_name[0]}
   rabbitmqctl start_app
   
fi
rabbitmqctl cluster_status
</script>
